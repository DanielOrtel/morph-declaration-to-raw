{
  "name": "morph-declaration-to-raw",
  "packageManager": "yarn@3.4.0",
  "description": "A ts-morph utility for transforming a Typescript AST node into a JS structure in memory",
  "type": "module",
  "types": ".dist/index.d.ts",
  "version": "0.1.0",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./.dist/index.js"
    }
  },
  "files": [
    ".dist",
    "src"
  ],
  "dependencies": {
    "lodash": "^4.17.21",
    "ts-morph": "^24.0.0",
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.0",
    "@babel/eslint-parser": "^7.25.9",
    "@babel/helper-module-imports": "^7.25.9",
    "@babel/node": "^7.26.0",
    "@babel/plugin-proposal-export-default-from": "^7.25.9",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/js": "^9.15.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@rollup/pluginutils": "^5.1.3",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.10.1",
    "babel-plugin-import": "^1.13.8",
    "babel-plugin-module-resolver": "^5.0.2",
    "colors": "^1.4.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "rollup": "^4.29.1",
    "rollup-plugin-delete": "^2.1.0",
    "rollup-plugin-glob-import": "^0.5.0",
    "rollup-plugin-node-externals": "^8.0.0",
    "tslib": "^2.8.1",
    "tsx": "^4.19.2",
    "typescript-eslint": "^8.18.1",
    "typescript-transform-paths": "^3.5.2"
  },
  "scripts": {
    "lint": "eslint --ignore-pattern \\\"!**/.*\\\" .",
    "fix": "yarn lint --fix",
    "typecheck": "tsc --noemit",
    "test": "yarn fix && yarn typecheck",
    "test-ci": "yarn lint && yarn typecheck",
    "build": "rollup --config",
    "watch": "rollup --config --w"
  }
}
